<?xml version='1.0' encoding='US-ASCII'?>
<summary_report xmlns="report.xsd">

	<header>
		<!-- date that report is generated on -->
		<date></date>
		<!-- project name: read from settings.ini -->
		<project></project>
	</header>

	<!-- summarized counts across all project sites -->
	<summary>
		<subject_count>
			<!-- current value -->
			<total></total>
			<!-- previous week's value -->
			<previous></previous>
			<!-- change across the week -->
			<delta></delta>
		</subject_count>
		<tests>
			<test name="test1">
				<total></total>
				<previous_total></previous_total>
				<!-- change across the week -->
				<delta></delta>
			</test>
			<test name="test2">
				<total></total>
				<previous_total></previous_total>
				<!-- change across the week -->
				<delta></delta>
			</test>
		</tests>
	</summary>

	<sites>
		<site name="site1">
				<test name="test1">
					<!-- current number of tests imported over past time interval (usually a week) -->
					<current_run></current_run>
					<!-- past number of tests over each subsequent run -->
					<previous_runs>
						<date>
							<date></date>
							<value></value>
						</date>
						<date>
							<date></date>
							<value></value>
						</date>
						<date>
							<date></date>
							<value></value>
						</date>
					</previous_runs>
				</test>

				<test name="test2">
					<!-- current number of tests imported over past time interval (usually a week) -->
					<current_run></current_run>
					<!-- past number of tests over each subsequent run -->
					<previous_runs>
						<date>
							<date></date>
							<value></value>
						</date>
						<date>
							<date></date>
							<value></value>
						</date>
						<date>
							<date></date>
							<value></value>
						</date>
					</previous_runs>
				</test>
		</site>
	</sites>

</summary_report>
